{
    "manifest_version": 3,
    "name": "Phishing Detection Extension",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "notifications",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  